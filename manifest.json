{
	"manifest_version" : 2,

	"name" : "NoticeFeed-VITPune",
	"description" : "This chrome extension keeps you updated with latest notices posted on vit.edu .",
	"version" : "1.0",

	"browser_action": {
		"default_icon": {                    
		  "16": "assets/img/extension_icon.png",           
		  "24": "assets/img/extension_icon.png",           
		  "32": "assets/img/extension_icon.png",
		  "128": "assets/img/extension_icon.png"            
		},
		"default_title": "VIT-NoticeFeed",      
		"default_popup": "popup.html"      
	  },	

	"background":
	{
		"page" : "background.html"
	},

	"permissions" : 
	[
		"storage",
		"notifications",
		"http://vit.edu/index.php/news/latest-news"
	],

	"content_security_policy": "script-src 'self'; object-src 'self'"
}
